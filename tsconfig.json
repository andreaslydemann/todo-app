{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src" /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "@api/*": ["api/*"],
      "@generated-api/*": ["generated-api/*"],
      "@screens/*": ["screens/*"],
      "@hooks/*": ["hooks/*"],
    },
  }
}
